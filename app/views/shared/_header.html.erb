<header id='site-header'>
  <% if current_user %>
    <%= current_user.email %>&nbsp;|
    <%= link_to 'Logout', user_session_path, method: :delete %>
  <% else %>
    <%= link_to 'Sign up', new_user_registration_path %>&nbsp;|
    <%= link_to 'Sign in', new_user_session_path %>
  <% end %>
</header>
